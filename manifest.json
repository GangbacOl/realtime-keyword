{
  "manifest_version": 2,
  "name": "실시간 검색어",
  "version": "1.0",
  "description": "실시간 검색어를 조회하는 크롬 확장 프로그램입니다.",
  "background": {
    "page": "./js/background/index.html",
    "persistent": true
  },
  "icons": {
    "128": "img/logo128.png",
    "48": "img/logo48.png",
    "16": "img/logo16.png"
  },
  "browser_action": {
    "default_icon": "img/logo16.png",
    "default_popup": "html/popup.html"
  },
  "permissions": ["storage", "*://trends.google.com/*"]
}
